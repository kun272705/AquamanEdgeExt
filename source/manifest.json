{
  "manifest_version": 3,
  "name": "AquamanEdgeExt",
  "version": "1.0.0",
  "description": "This is the AquamanEdgeExt.",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "./icons/icon16-off.png",
      "48": "./icons/icon48-off.png",
      "128": "./icons/icon128-off.png"
    }
  },
  "background": {
    "service_worker": "./Service-Worker.js",
    "type": "module"
  },
  "default_locale": "en_US",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "116",
  "permissions": [
    "tabs",
    "debugger",
    "scripting",
    "downloads"
  ]
}
