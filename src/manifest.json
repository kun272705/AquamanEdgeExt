{
  "manifest_version": 3,
  "name": "AquamanEdgeExt",
  "version": "1.0.0",
  "description": "This is AquamanEdgeExt.",
  "icons": {
    "16": "./images/icon-16.png",
    "48": "./images/icon-48.png",
    "128": "./images/icon-128.png"
  },
  "action": {
    "default_icon": {
      "16": "./images/action-icon-16-off.png",
      "48": "./images/action-icon-48-off.png",
      "128": "./images/action-icon-128-off.png"
    }
  },
  "background": {
    "service_worker": "./background.js",
    "type": "module"
  },
  "default_locale": "en_US",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "minimum_chrome_version": "116",
  "permissions": [
    "tabs",
    "debugger",
    "scripting",
    "downloads",
    "power"
  ]
}
