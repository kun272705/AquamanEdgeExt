
{
  "manifest_version": 3,
  "name": "Aquaman",
  "version": "1.0.0",
  "description": "This is the Aquaman Edge Ext.",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "./icons/icon16-off.png",
      "48": "./icons/icon48-off.png",
      "128": "./icons/icon128-off.png"
    }
  },
  "background": {
    "service_worker": "./service-worker.js",
    "type": "module"
  },
  "default_locale": "en_US",
  "homepage_url": "https://aquaman.kun272705.com/",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "debugger",
    "nativeMessaging",
    "scripting",
    "downloads"
  ]
}
