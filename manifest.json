{
  "manifest_version": 3,
  "name": "Aquaman Browser Ext",
  "version": "0.1.0",
  "description": "This is the Aquaman Browser Ext.",
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": {
  },
  "background": {
    "service_worker": "./service-worker.js",
    "type": "module"
  },
  "default_locale": "en_US",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "debugger",
    "nativeMessaging",
    "scripting",
    "downloads"
  ]
}
